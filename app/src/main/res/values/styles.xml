<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- AppTheme style removed to resolve duplicate resource error. -->

    <style name="ShapeAppearance.TopEndRounded48Dp" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">48dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>

    <style name="Widget.GoalGetters.Chip.Filled" parent="Widget.MaterialComponents.Chip.Entry">
        <item name="chipCornerRadius">16dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="chipBackgroundColor">@color/primary</item>
        <item name="chipStrokeColor">@color/primary</item>
        <item name="chipStrokeWidth">1dp</item>
        <item name="android:fontFamily">@font/variable_inter</item>
        <item name="android:textSize">16sp</item>
        <item name="chipIconTint">@color/black</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="Widget.GoalGetters.Chip.Outlined" parent="Widget.MaterialComponents.Chip.Entry">
        <item name="chipCornerRadius">16dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="chipBackgroundColor">@color/white</item>
        <item name="chipStrokeColor">@color/primary</item>
        <item name="chipStrokeWidth">2dp</item>
        <item name="android:fontFamily">@font/variable_inter</item>
        <item name="android:textSize">16sp</item>
        <item name="chipIconTint">@color/primary</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="Widget.GoalGetters.Button.Filled" parent="Widget.MaterialComponents.Button">
        <item name="cornerRadius">16dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="backgroundTint">@color/primary</item>
        <item name="android:fontFamily">@font/variable_inter</item>
        <item name="android:textSize">16sp</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="Widget.GoalGetters.Button.Outlined" parent="Widget.MaterialComponents.Button">
        <item name="cornerRadius">16dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/primary</item>
        <item name="strokeColor">@color/primary</item>
        <item name="strokeWidth">2dp</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:fontFamily">@font/variable_inter</item>
        <item name="android:textSize">16sp</item>
        <item name="elevation">0dp</item>
    </style>
</resources> 